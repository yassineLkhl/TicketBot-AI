{
  "devDependencies": {
    "prisma": "^6.13.0",
    "ts-node": "^10.9.2",
    "@types/node": "^22.10.7",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "seed": "ts-node prisma/seed.ts",
    "migrate:dev": "prisma migrate dev --schema ../../prisma/schema.prisma --env-file=../../.env.development",
    "migrate:staging": "prisma migrate deploy --schema ../../prisma/schema.prisma --env-file=../../.env.staging",
    "migrate:prod": "prisma migrate deploy --schema ../../prisma/schema.prisma --env-file=../../.env.production"
  }
}